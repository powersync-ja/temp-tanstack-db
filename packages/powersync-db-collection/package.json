    "directory": "packages/powersync-db-collection"
  },
  "homepage": "https://tanstack.com/db",
  "keywords": [
    "powersync",
    "realtime",
    "local-first",
    "sync-engine",
    "sync",
    "replication",
    "opfs",
    "indexeddb",
    "localstorage",
    "optimistic",
    "typescript"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "lint": "eslint . --fix",
    "test": "npx vitest --run"
  },
  "sideEffects": false,
  "type": "module",
  "types": "dist/esm/index.d.ts"
